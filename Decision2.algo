FUNCTION function_movieTicketPrice() 
VAR
    price, age: INTEGER;

BEGIN
   display "Please enter your age:"
   read age

    IF (age <= 12) THEN
        price = 10
    ELSE_IF (age >= 13 and age <= 17) THEN
        price = 15
    ELSE
        price = 20
    END_IF   
    display "The price of the movie ticket is $" + price + "."
   END_IF
    RETURN value ;
END


